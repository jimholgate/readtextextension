	if fsGetOS = &quot;WINDOWS&quot; and _
			Ucase(Environ(&quot;PROCESSOR_ARCHITECTURE&quot;)) = &quot;AMD64&quot; and _
			instr(lcase(installPath), &quot;(x86)&quot;) &lt; 1 then
		fbIsWin64Program = true
