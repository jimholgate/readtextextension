		fbHelpIsInstalled=True
	Elseif fileExists(sA &amp; &quot;en&quot;) Then
		fbHelpIsInstalled=True
	Elseif fileExists(sA &amp; &quot;fr&quot;) Then
		fbHelpIsInstalled=True
	Elseif fileExists(sA &amp; &quot;de&quot;) Then
		fbHelpIsInstalled=True
	Else
