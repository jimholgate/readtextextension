Function fsGetPicturePath() As String
	Dim oDialog
	Dim oAllFiles
	Dim oFrame
	Dim oBar
	Dim sFilePickerArgs
	
	On Error Goto fsGetPicturePathErr
	oDialog = createUnoService(&quot;com.sun.star.ui.dialogs.FilePicker&quot;)
