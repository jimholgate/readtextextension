		_lang = fsSelectionLanguageAndRegion(False)
	End If
	PiperLocalSuggestion = &quot;en_GB-jenny_dioco-medium&quot;
	Dim models As Variant
	If _high Then
		PiperLocalSuggestion = &quot;en_US-lessac-high&quot;
