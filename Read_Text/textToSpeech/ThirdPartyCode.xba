Sub ChangeBackground( _
	ByVal i, _
	ByVal r1, _
	ByVal g1, _
	ByVal b1)
	&apos;AndrewMacro.odt - [Andrew Pitonyak](http://www.pitonyak.org)
	Dim oDoc As object
	Dim oDrawPages As object
	Dim oDrawPage As object
	Dim oBackground As object
	
	oDoc = ThisComponent
